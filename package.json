{
  "name": "@eldarlabs/cycle-ui-typescript-webpack",
  "version": "0.0.0-semantically-released-after-ci-build",
  "description": "Starter project with Cycle-UI, TypeScript and WebPack",
  "author": "Eldar Labs (https://eldarlabs.com)",
  "license": "MIT",
  "main": "index.ts",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eldarlabs/cycle-ui-typescript-webpack.git"
  },
  "bugs": {
    "url": "https://github.com/eldarlabs/cycle-ui-typescript-webpack/issues"
  },
  "scripts": {
    "postinstall": "typings install",
    "commit": "git-cz",
    "tsc": "tsc",
    "typings": "typings",
    "tslint": "tslint",
    "start": "node server.js",
    "build": "NODE_ENV=prod webpack --progress --colors --config webpack.config.js",
    "stage": "NODE_ENV=stage webpack --progress --colors --config webpack.config.js",
    "clean": "rm -rf build/",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "release": {
    "verifyConditions": "condition-circle"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "cycle-ui",
    "typescript",
    "webpack",
    "cycle.js",
    "starter-kit",
    "boilerplate",
    "reactive",
    "rxjs",
    "snabbdom",
    "virtual-dom"
  ],
  "dependencies": {
    "@cycle/core": "^6.0.0",
    "@cycle/isolate": "1.2.0",
    "@eldarlabs/cycle-ui": "0.6.0",
    "autoprefixer": "^6.3.4",
    "classnames": "^2.2.3",
    "cycle-snabbdom": "1.2.1",
    "normalize.css": "^3.0.3",
    "react-toolbox": "0.16.2",
    "rx": "^4.1.0"
  },
  "devDependencies": {
    "commitizen": "2.7.6",
    "condition-circle": "1.2.0",
    "css-loader": "^0.23.0",
    "cz-conventional-changelog": "1.1.5",
    "extract-text-webpack-plugin": "^0.9.1",
    "html-webpack-plugin": "2.15.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "path": "^0.11.14",
    "postcss-loader": "^0.8.1",
    "sass-loader": "^3.1.2",
    "semantic-release": "4.3.5",
    "style-loader": "^0.13.0",
    "toolbox-loader": "0.0.3",
    "ts-loader": "^0.8.1",
    "tslint": "^3.5.0",
    "typescript": "^1.8.7",
    "typings": "0.7.11",
    "webpack": "^1.10.1",
    "webpack-config": "4.0.0",
    "webpack-dev-server": "1.14.1"
  }
}
